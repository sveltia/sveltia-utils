{
  "name": "@sveltia/utils",
  "version": "0.7.3",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "github:sveltia/sveltia-utils"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package",
    "build:watch": "svelte-kit sync && svelte-package --watch",
    "preview": "vite preview",
    "prepublishOnly": "npm run build",
    "format": "prettier --plugin prettier-plugin-svelte --write .",
    "check": "pnpm run '/^check:.*/'",
    "check:audit": "pnpm audit",
    "check:cspell": "cspell --no-progress",
    "check:svelte": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:prettier": "prettier --check --plugin prettier-plugin-svelte .",
    "check:eslint": "eslint .",
    "check:stylelint": "stylelint '**/*.{css,scss,svelte}'",
    "test:unit": "vitest"
  },
  "dependencies": {
    "base32-encode": "^2.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/kit": "^2.20.7",
    "@sveltejs/package": "^2.3.11",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@types/node": "^22.15.2",
    "cspell": "^8.19.2",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.11",
    "eslint-plugin-svelte": "^2.46.1",
    "fake-indexeddb": "^6.0.0",
    "postcss": "^8.5.3",
    "postcss-html": "^1.8.0",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "sass": "^1.87.0",
    "stylelint": "^16.19.1",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-scss": "^6.11.1",
    "svelte": "^5.28.2",
    "svelte-check": "^4.1.6",
    "svelte-i18n": "^4.0.1",
    "svelte-preprocess": "^6.0.3",
    "tslib": "^2.8.1",
    "vite": "^6.3.3",
    "vitest": "^3.1.2"
  },
  "files": [
    "dist",
    "!**/*.spec.js",
    "!**/*.spec.d.ts"
  ],
  "exports": {
    "./array": {
      "types": "./dist/array.d.ts",
      "default": "./dist/array.js"
    },
    "./crypto": {
      "types": "./dist/crypto.d.ts",
      "default": "./dist/crypto.js"
    },
    "./datetime": {
      "types": "./dist/datetime.d.ts",
      "default": "./dist/datetime.js"
    },
    "./element": {
      "types": "./dist/element.d.ts",
      "default": "./dist/element.js"
    },
    "./file": {
      "types": "./dist/file.d.ts",
      "default": "./dist/file.js"
    },
    "./misc": {
      "types": "./dist/misc.d.ts",
      "default": "./dist/misc.js"
    },
    "./object": {
      "types": "./dist/object.d.ts",
      "default": "./dist/object.js"
    },
    "./storage": {
      "types": "./dist/storage/index.d.ts",
      "svelte": "./dist/storage/index.js",
      "default": "./dist/storage/index.js"
    },
    "./string": {
      "types": "./dist/string.d.ts",
      "default": "./dist/string.js"
    }
  },
  "typesVersions": {
    ">4.0": {
      "array": [
        "./dist/array.d.ts"
      ],
      "crypto": [
        "./dist/crypto.d.ts"
      ],
      "datetime": [
        "./dist/datetime.d.ts"
      ],
      "element": [
        "./dist/element.d.ts"
      ],
      "file": [
        "./dist/file.d.ts"
      ],
      "misc": [
        "./dist/misc.d.ts"
      ],
      "object": [
        "./dist/object.d.ts"
      ],
      "storage": [
        "./dist/storage/index.d.ts"
      ],
      "string": [
        "./dist/string.d.ts"
      ]
    }
  },
  "pnpm": {
    "overrides": {
      "cookie@<0.7.0": ">=0.7.0",
      "esbuild@<=0.24.2": ">=0.25.0"
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "svelte-preprocess"
    ]
  }
}
