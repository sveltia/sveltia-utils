{
  "name": "@sveltia/utils",
  "version": "0.8.5",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "github:sveltia/sveltia-utils"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package",
    "build:watch": "svelte-kit sync && svelte-package --watch",
    "preview": "vite preview",
    "prepublishOnly": "npm run build",
    "format": "prettier --plugin prettier-plugin-svelte --write .",
    "check": "pnpm run '/^check:.*/'",
    "check:audit": "pnpm audit",
    "check:cspell": "cspell --no-progress",
    "check:svelte": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:prettier": "prettier --check --plugin prettier-plugin-svelte .",
    "check:eslint": "eslint .",
    "check:stylelint": "stylelint '**/*.{css,scss,svelte}'",
    "test:unit": "vitest"
  },
  "dependencies": {
    "base32-encode": "^2.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/kit": "^2.47.3",
    "@sveltejs/package": "^2.5.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/node": "^24.9.1",
    "cspell": "^9.2.2",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^56.1.2",
    "eslint-plugin-svelte": "^2.46.1",
    "fake-indexeddb": "^6.2.4",
    "jsdom": "^27.0.1",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "sass": "^1.93.2",
    "stylelint": "^16.25.0",
    "stylelint-config-recommended-scss": "^16.0.2",
    "stylelint-scss": "^6.12.1",
    "svelte": "^5.41.0",
    "svelte-check": "^4.3.3",
    "svelte-i18n": "^4.0.1",
    "svelte-preprocess": "^6.0.3",
    "tslib": "^2.8.1",
    "vite": "^7.1.12",
    "vitest": "^4.0.3"
  },
  "files": [
    "dist",
    "!**/*.spec.js",
    "!**/*.spec.d.ts"
  ],
  "exports": {
    "./array": {
      "types": "./dist/array.d.ts",
      "default": "./dist/array.js"
    },
    "./crypto": {
      "types": "./dist/crypto.d.ts",
      "default": "./dist/crypto.js"
    },
    "./datetime": {
      "types": "./dist/datetime.d.ts",
      "default": "./dist/datetime.js"
    },
    "./element": {
      "types": "./dist/element.d.ts",
      "default": "./dist/element.js"
    },
    "./file": {
      "types": "./dist/file.d.ts",
      "default": "./dist/file.js"
    },
    "./misc": {
      "types": "./dist/misc.d.ts",
      "default": "./dist/misc.js"
    },
    "./object": {
      "types": "./dist/object.d.ts",
      "default": "./dist/object.js"
    },
    "./storage": {
      "types": "./dist/storage/index.d.ts",
      "svelte": "./dist/storage/index.js",
      "default": "./dist/storage/index.js"
    },
    "./string": {
      "types": "./dist/string.d.ts",
      "default": "./dist/string.js"
    }
  },
  "typesVersions": {
    ">4.0": {
      "array": [
        "./dist/array.d.ts"
      ],
      "crypto": [
        "./dist/crypto.d.ts"
      ],
      "datetime": [
        "./dist/datetime.d.ts"
      ],
      "element": [
        "./dist/element.d.ts"
      ],
      "file": [
        "./dist/file.d.ts"
      ],
      "misc": [
        "./dist/misc.d.ts"
      ],
      "object": [
        "./dist/object.d.ts"
      ],
      "storage": [
        "./dist/storage/index.d.ts"
      ],
      "string": [
        "./dist/string.d.ts"
      ]
    }
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
